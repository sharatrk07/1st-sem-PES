    "class Vehicle:\n",
    "    def __init__(self, make, model, year):\n",
    "        self.make = make\n",
    "        self.model = model\n",
    "        self.year = year\n",
    "    \n",
    "    def disp(self):\n",
    "        print(f\"{self.make}\\n{self.model}\\n{self.year}\")\n",
    "\n",
    "v = Vehicle('Toyota', 'Fortuner', 2023)\n",
    "v.disp()"
   ]
